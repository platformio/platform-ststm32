{
  "build": {
    "cpu": "cortex-m7",
    "extra_flags": "-DSTM32F7 -DSTM32F765xx",
    "f_cpu": "216000000L",
    "framework_extra_flags": {
      "arduino": "-DCUSTOM_PERIPHERAL_PINS"
    },
    "mcu": "stm32f765vit6",
    "product_line": "STM32F765xx",
    "variant": "STM32F7xx/F765V(G-I)(H-T)_F767V(G-I)(H-T)_F777VI(H-T)"
  },
  "connectivity": [
    "can"
  ],
  "debug": {
    "default_tools": [
      "stlink"
    ],
    "jlink_device": "STM32F765VI",
    "onboard_tools": [
      "stlink"
    ],
    "openocd_target": "stm32f7x",
    "svd_path": "STM32F765.svd"
  },
  "frameworks": [
    "arduino",
    "cmsis",
    "stm32cube",
    "libopencm3"
  ],
  "name": "3D printer controller",
  "upload": {
    "maximum_ram_size": 524288,
    "maximum_size": 2097152,
    "protocol": "stlink",
    "protocols": [
      "jlink",
      "cmsis-dap",
      "stlink",
      "blackmagic",
      "serial"
    ]
  },
  "url": "https://github.com/hasenbanck/remram",
  "vendor": "RemRam"
}
