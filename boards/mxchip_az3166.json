{
  "build": {
    "core": "arduino",
    "cpu": "cortex-m4",
    "extra_flags": "-DSTM32F412Zx -DARDUINO_MXCHIP_AZ3166 -DARDUINO_ARCH_STM32F4",
    "f_cpu": "100000000L",
    "ldscript": "AZ3166.ld ",
    "mcu": "stm32f412zgt6",
    "variant": "MXChip_AZ3166"
  },
  "connectivity": [
    "can",
    "wifi"
  ],
  "debug": {
    "default_tools": [
      "stlink"
    ],
    "jlink_device": "STM32F412ZG",
    "onboard_tools": [
      "stlink"
    ],
    "openocd_target": "stm32f4x",
    "svd_path": "STM32F41x.svd"
  },
  "frameworks": [
    "arduino",
    "stm32cube"
  ],
  "name": "Microsoft Azure IoT Development Kit (MXChip AZ3166)",
  "upload": {
    "maximum_ram_size": 262144,
    "maximum_size": 1048576,
    "offset_address": "0x0800C000",
    "protocol": "stlink",
    "protocols": [
      "jlink",
      "stlink",
      "blackmagic"
    ]
  },
  "url": "https://microsoft.github.io/azure-iot-developer-kit/",
  "vendor": "MXChip"
}
