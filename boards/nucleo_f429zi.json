{
  "build": {
    "cpu": "cortex-m4",
    "extra_flags": "-DSTM32F429xx",
    "f_cpu": "180000000L",
    "mcu": "stm32f429zit6"
  },
  "connectivity": [
    "can",
    "ethernet"
  ],
  "frameworks": [
    "mbed",
    "stm32cube"
  ],
  "name": "ST Nucleo F429ZI",
  "upload": {
    "maximum_ram_size": 262144,
    "maximum_size": 2097152
  },
  "debug": {
    "tools": {
      "stlink-v2-1": {
        "server": {
          "package": "tool-openocd",
          "executable": "bin/openocd",
          "arguments": ["-f", "scripts/board/st_nucleo_f4.cfg"]
        },
        "onboard": true
      }
    }
  },
  "url": "https://developer.mbed.org/platforms/ST-Nucleo-F429ZI/",
  "vendor": "ST"
}