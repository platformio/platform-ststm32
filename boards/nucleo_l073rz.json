{
  "build": {
    "cpu": "cortex-m0plus",
    "extra_flags": "-DSTM32L073xx",
    "f_cpu": "32000000L",
    "mcu": "stm32l073rz"
  },
  "frameworks": [
    "mbed",
    "stm32cube"
  ],
  "name": "ST Nucleo L073RZ",
  "upload": {
    "maximum_ram_size": 20480,
    "maximum_size": 196608,
    "protocol": "mbed",
    "protocols": [
      "jlink",
      "stlink",
      "blackmagic",
      "mbed"
    ]
  },
  "debug": {
    "tools": {
      "stlink-v2-1": {
        "server": {
          "package": "tool-openocd",
          "executable": "bin/openocd",
          "arguments": ["-f", "scripts/board/st_nucleo_l073rz.cfg"]
        },
        "onboard": true
      }
    }
  },
  "url": "https://developer.mbed.org/platforms/ST-Nucleo-L073RZ/",
  "vendor": "ST"
}