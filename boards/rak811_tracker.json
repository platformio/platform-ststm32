{
  "build": {
    "cpu": "cortex-m3",
    "extra_flags": "-DSTM32L1 -DSTM32L151xB",
    "f_cpu": "32000000L",
    "framework_extra_flags": {
      "arduino": "-DCUSTOM_PERIPHERAL_PINS"
    },
    "mcu": "stm32l151rbt6",
    "product_line": "STM32L151xB",
    "variant": "STM32L1xx/L100C6Ux(A)_L151C(6-8-B)(T-U)x(A)_L152C(6-8-B)(T-U)x(A)"
  },
  "debug": {
    "jlink_device": "STM32L151CC",
    "openocd_target": "stm32l1",
    "svd_path": "STM32L1xx.svd"
  },
  "frameworks": [
    "arduino",
    "cmsis",
    "stm32cube",
    "libopencm3"
  ],
  "name": "RAK811 LoRa Tracker",
  "upload": {
    "maximum_ram_size": 16384,
    "maximum_size": 131072,
    "protocol": "stlink",
    "protocols": [
      "jlink",
      "cmsis-dap",
      "stlink",
      "blackmagic",
      "serial"
    ]
  },
  "url": "https://github.com/RAKWireless/RAK811",
  "vendor": "RAK"
}
