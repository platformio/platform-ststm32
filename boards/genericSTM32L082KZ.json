  },
  "frameworks": [
    "arduino",
    "mbed",
    "stm32cube"
  ],
  "name": "STM32L082KZ",
  "upload": {
    "maximum_ram_size": 20480,
    "maximum_size": 196608,
    "protocol": "stlink",
