{
  "build": {
    "cpu": "cortex-m0plus",
    "extra_flags": "-DSTM32L082xx",
    "f_cpu": "32000000L",
    "framework_extra_flags": {
      "arduino": "-D__CORTEX_SC=0"
    },
    "mcu": "stm32l080kzt6",
    "variant": "STM_L082KZ"
  },
  "debug": {
    "default_tools": [
      "stlink"
    ],
    "jlink_device": "STM32L080KZ",
