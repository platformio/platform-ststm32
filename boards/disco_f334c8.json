{
  "build": {
    "cpu": "cortex-m4",
    "f_cpu": "72000000L",
    "mcu": "stm32f334c8t6"
  },
  "frameworks": [
    "mbed"
  ],
  "name": "ST 32F3348DISCOVERY",
  "upload": {
    "maximum_ram_size": 12288,
    "maximum_size": 65536,
    "protocol": "stlink"
  },
  "debug": {
    "tools": {
      "stlink-v2-1": {
        "server": {
          "package": "tool-openocd",
          "executable": "bin/openocd",
          "arguments": ["-f", "scripts/board/stm32f334discovery.cfg"]
        },
        "gdbinit": [
          "target remote :3333",
          "file $PROG_PATH",
          "load $PROG_PATH",
          "monitor reset halt",
          "monitor reset init",
          "b main"
        ],
        "onboard": true
      }
    }
  },
  "url": "http://www.st.com/web/en/catalog/tools/PF260318",
  "vendor": "ST"
}